{
  "name": "uniroster-server",
  "description": "Rostering server supporting multiple protocols (e.g., OneRoster, Ed-Fi, etc.)",
  "version": "1.0.0",
  "main": "uniroster-server.js",
  "keywords": [
    "ims",
    "oneroster",
    "edfi",
    "ed-fi",
    "roster",
    "sif",
    "xpress-roster"
  ],
  "scripts": {
    "start": "node ./bin/www",
    "start:nodemon": "nodemon ./bin/www --trace-events-enabled",
    "start:dev": "docker-compose up -d && npm run init-db:dev",
    "init-db:dev": "docker exec -it mariadb /uniroster-server/sql/dev_setup.sh"
  },
  "dependencies": {
    "body-parser": "~1.15.1",
    "chance": "~1.0.4",
    "cookie-parser": "~1.4.3",
    "date-format": "~0.0.2",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "jade": "~1.11.0",
    "morgan": "~1.7.0",
    "mysql": "~2.11.1",
    "oauth-signature": "~1.3.1",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
