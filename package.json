{
  "name": "uniroster-server",
  "version": "1.0.0",
  "description": "Rostering server supporting multiple protocols (e.g., OneRoster, Ed-Fi, etc.)",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "start:dev": "docker-compose up -d && npm run init-db:dev",
    "init-db:dev": "docker exec -it mysql /uniroster-server/sql/dev_setup.sh"
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "express": "4.17.1",
    "express-oauth-server": "2.0.0",
    "oauth2-server": "3.1.1",
    "promise-mysql": "4.1.3"
  }
}
